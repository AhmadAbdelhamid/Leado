<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home_Fragment">
    <fragment
        android:id="@+id/home_Fragment"
        android:name="com.leado.ui.navBarFragments.Home.HomeScrollFragment"
        android:label="HomeFragment"
        tools:layout="@layout/home_scrolled_fragment">
        <action
            android:id="@+id/action_home_Fragment_to_nav_journey"
            app:destination="@id/nav_journey" />
    </fragment>
    <fragment
        android:id="@+id/search_Fragment"
        android:name="com.leado.ui.navBarFragments.Search.SearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/search_fragment"
        />
    <fragment
        android:id="@+id/leaderBoard_Fragment"
        android:name="com.leado.ui.navBarFragments.LeaderBoard.AchievementsFragment"
        android:label="LeaderBoardFragment"
        tools:layout="@layout/achievments_page"
        />
    <fragment
        android:id="@+id/profile_Fragment"
        android:name="com.leado.ui.navBarFragments.Profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/profile_fragment"
        />

    <include app:graph="@navigation/nav_journey" />


</navigation>